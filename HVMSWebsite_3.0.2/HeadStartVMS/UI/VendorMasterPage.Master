    function HideDocumentpopup11092012(inventoryID) {
        //var InventoryId = $('#inventoryID').val();
        //$find('DocumentModelPopup11').hide();
        document.getElementById('ctl00_divScan').value;

        window.location.href = '../UI/InventoryDocuments.aspx?Code=' + inventoryID;
        return false;
    }

    function validateScanVIN() {
        var txt = document.getElementById('ctl00_txtScanVIN').value;
        if (txt == '') {
            alert("Please enter a VIN");
            return false;
        }
        else
            return true;
    }
</script>
